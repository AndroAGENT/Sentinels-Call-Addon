{
	"$schema": "https://raw.githubusercontent.com/Bedrock-OSS/regolith-schemas/main/config/v1.2.json",
	"author": "Andro AGENT",
	"name": "SentinelsCall",
	"packs": {
		"behaviorPack": "./packs/BP",
		"resourcePack": "./packs/RP"
	},
	"regolith": {
		"dataPath": "./packs/data",
		"filterDefinitions": {
			"blockbench_convert": {
				"url": "github.com/Bedrock-OSS/regolith-filters",
				"version": "1.1.1"
			},
			"bump_manifest": {
				"url": "github.com/Bedrock-OSS/regolith-filters",
				"version": "1.0.0"
			},
			"name_ninja": {
				"url": "github.com/Bedrock-OSS/regolith-filters",
				"version": "1.2.3"
			},
			"texture_list": {
				"url": "github.com/Bedrock-OSS/regolith-filters",
				"version": "1.1.3"
			}
		},
		"profiles": {
			"default": {
				"export": {
					"readOnly": false,
					"target": "development"
				},
				"filters": [
					{
						"filter": "blockbench_convert"
					},
					{
						"filter": "bump_manifest"
					},
					{
						"filter": "name_ninja"
					},
					{
						"filter": "texture_list"
					}
				]
			}
		}
	}
}